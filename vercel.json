{
  "functions": {
    "api/cron.js": {
      "maxDuration": 60
    },
    "api/logs.js": {
      "maxDuration": 30
    },
    "api/daily-checkin.js": {
      "maxDuration": 30
    },
    "api/daily-checkin-cron.js": {
      "maxDuration": 30
    },
    "api/cleaning-plan.js": {
      "maxDuration": 30
    },
    "api/cleaning-cron.js": {
      "maxDuration": 60
    },
    "api/cleaning-plan-page.js": {
      "maxDuration": 30
    },
    "api/daily-checkin-page.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/daily-checkin",
      "destination": "/api/daily-checkin-page"
    },
    {
      "source": "/daily-checkin/app.js",
      "destination": "/api/daily-checkin-page"
    },
    {
      "source": "/cleaning-plan",
      "destination": "/api/cleaning-plan-page"
    },
    {
      "source": "/cleaning-plan/app.js",
      "destination": "/api/cleaning-plan-page"
    }
  ]
}
